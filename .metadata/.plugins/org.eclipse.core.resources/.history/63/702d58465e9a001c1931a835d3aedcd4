import java.util.Scanner;

public class Mainptr10 {
public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	System.out.println("Enter the values of N");
	int n=sc.nextInt();
	for(int i=1;i<=n;i++)
	{
		for(int j=1;j<=n;j++)
		{
			System.out.print(i%2+" ");
		}
		System.out.println();
	}
}
}
